   	PO.DOC_ID    	  		PO_DOC_ID,
	PO.PURCHASER      		PO_PURCHASER,
	PO.IS_PREAPPROVED		PO_IS_PREAPPROVED,
	M.MATERIAL_ID     		M_MATERIAL_ID,
	M.NAME			  		M_NAME,
	POL.UOM_ID     	  		POL_UOM_ID,
	POL.QTY      	 	 	POL_QTY,
	POL.UNIT_PRICE  	  	POL_UNIT_PRICE,
	POL.QTY_IN      	  	POL_QTY_IN,
	POL.DATE_END      		POL_DATE_END,
	POL.URGENCY				POL_URGENCY,
	PO.DELIVERY_RULE  		PO_DELIVERY_RULE,
	PO.DELIVERY_ADDRESS     PO_DELIVERY_ADDRESS,
	PO.COMMENTS     	 	PO_COMMENTS,
	PO.USER_CREATED     	PO_USER_CREATED,
	PO.CREATED      		PO_CREATED,
	PO.UPDATED_BY      		PO_UPDATED_BY,
	PO.UPDATED      		PO_UPDATED,
	PO.PRINT_TIME			PO_PRINT_TIME,
	PO.PAYMENT_RULE1		PAYMENT_RULE1,
	PO.PAYMENT_RULE2		PAYMENT_RULE2,
	PO.PAYMENT_RULE3		PAYMENT_RULE3,
	PO.PAYMENT_RULE4		PAYMENT_RULE4,
	PO.PAYMENT_RULE5		PAYMENT_RULE5,
	PO.PAYMENT_RULE6		PAYMENT_RULE6,
	PO.PAYMENT_RULE7		PAYMENT_RULE7,
	PO.PAYMENT_RULE8		PAYMENT_RULE8,
	PO.PAYMENT_RULE9		PAYMENT_RULE9,
	PO.PAYMENT_RULE10		PAYMENT_RULE10,
	PO.PAYMENT_RULE11		PAYMENT_RULE11,
	PO.PAYMENT_RULE12		PAYMENT_RULE12,
	PO.PAYMENT_RULE13		PAYMENT_RULE13,
	PO.PAYMENT_RULE14		PAYMENT_RULE14,
	PO.PAYMENT_RULE15		PAYMENT_RULE15,
	PO.QUALITY_RULE1		QUALITY_RULE1,
	PO.QUALITY_RULE2		QUALITY_RULE2,
	PO.QUALITY_RULE3		QUALITY_RULE3,
	PO.QUALITY_RULE4		QUALITY_RULE4,
	PO.QUALITY_RULE5		QUALITY_RULE5,
	PO.INVOICE_TYPE			INVOICE_TYPE,
	PO.VAT_RATE				VAT_RATE,
	NVL(PO.PREAPPROVED,'')	PREAPPROVED,
	NVL(U1.DESCRIPTION,'')	USER_NAME_PREAPPROVED,
	NVL(PO.DATE_PREAPPROVED,'') DATE_PREAPPROVED,
	NVL(PO.USER_APPROVED,'')	USER_APPROVED,
	NVL(U2.DESCRIPTION,'')		USER_NAME_APPROVED,
	NVL(PO.DATE_APPROVED,'')	DATE_APPROVED,
	NVL(PO.REQUISITION_ID,'')	REQUISITION_ID,
	
	PR.MPS_RRN				MPS_RRN,
	
	V.VENDOR_ID||'  '||NVL(V.COMPANY_NAME,' ')	VENDOR,
	V.CONTACT				V_CONTACT,--联系人
	V.PHONE1				V_PHONE1,--电话1
	V.PHONE2				V_PHONE2,--电话2
	V.FAX					V_FAX--传真

from PUR_PO   PO,
    PUR_PO_LINE  POL,
    PUR_REQUISITION PR,
    PDM_MATERIAL M,
    AD_USER	U1,
    AD_USER U2,
    VDM_VENDOR  V
where PO.OBJECT_RRN = ? 
    and POL.PO_RRN = PO.OBJECT_RRN
    and POL.LINE_STATUS &lt;> 'CLOSED'
    and M.OBJECT_RRN = POL.MATERIAL_RRN
    and V.OBJECT_RRN = PO.VENDOR_RRN
    and PR.OBJECT_RRN(+) = PO.REQUISITION_RRN
    and U1.USER_NAME(+) = PO.PREAPPROVED
    and U2.USER_NAME(+) = PO.USER_APPROVED
order by M_MATERIAL_ID,POL_DATE_END

</property>
