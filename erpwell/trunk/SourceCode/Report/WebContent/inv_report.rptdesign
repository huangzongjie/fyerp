            <property name="queryText">select 
		V.COMPANY_NAME			COMPANY_NAME,
		R.PO_ID					PO_ID,
		R.DOC_ID				DOC_ID,
		P.OBJECT_RRN			PO_RRN,
		P.COMMENTS				COMMENTS,
		M.MATERIAL_ID			MATERIAL_ID,
		M.NAME					NAME,
		L.UOM_ID				UOM_ID,
		L.QTY_RECEIPT			QTY_RECEIPT,
		L.DESCRIPTION			DESCRIPTION,
		R.USER_CREATED			USER_CREATED,
		R.CREATED				CREATED,
		PL.URGENCY     			URGENCY		
from  	INV_RECEIPT			R,
		INV_RECEIPT_LINE	L,
		PUR_PO				P,
		VDM_VENDOR			V,
		PDM_MATERIAL		M,
		PUR_PO_LINE         PL
where R.OBJECT_RRN=? 
	and P.OBJECT_RRN = R.PO_RRN
	and V.OBJECT_RRN = P.VENDOR_RRN
	and L.RECEIPT_RRN=R.OBJECT_RRN
	and L.MATERIAL_RRN=M.OBJECT_RRN
	and L.PO_LINE_RRN=PL.OBJECT_RRN
	--and M.IS_INSPECTION_FREE = 'N'</property>
